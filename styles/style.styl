@media (min-width: 468px) and (max-width: 1024px) and (min-height: 60px) and (max-height: 90px)
	@import '468x60'
@media (min-width: 728px) and (max-width: 1024px) and (min-height: 90px) and (max-height: 90px)
	@import '728x90'
@media (min-width: 1024px) and (max-width: 1024px) and (min-height: 90px) and (max-height: 90px)
	@import '1024x90'


@media (min-width: 300px) and (max-width: 480px) and (min-height: 250px) and (max-height: 480px)
	@import '300x250'
@media (min-width: 320px) and (max-width: 320px) and (min-height: 480px) and (max-height: 480px)
	@import '320x480'
@media (min-width: 336px) and (max-width: 336px) and (min-height: 280px) and (max-height: 280px)
